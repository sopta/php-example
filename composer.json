{
  "require": {
    "slim/slim": "^4.0",
    "slim/psr7": "^1.7",
    "league/fractal": "^0.20.1",
    "league/container": "^4.2",
    "maximebf/debugbar": "^1.22"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Example\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
